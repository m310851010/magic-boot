return db.page("""
    select id,value,label,dict_id,remarks,sort from sys_dict_items
    where is_del = 0
    ?{label, and label like concat('%',#{label},'%')}
    ?{dictId, and dict_id = #{dictId}}
    ?{value, and value like concat('%',#{value},'%')}
    order by sort
""")